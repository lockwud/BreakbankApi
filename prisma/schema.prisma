// schema.prisma

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL") // define your DB connection string in .env file
}

model user {
    id String @id @default(uuid())
    fullname String
    email String @unique
    password String
    token String?
    tokenExpiry DateTime?
    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt
}

model fileUpload {
    id String @id @default(uuid())
    title String
    year DateTime
    file    String[]
    examType examType
    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt
}



enum examType {
    midsem
    final
    quiz
    other
    assignment
    practicequestions
    national
    entrance
}